{
  "name": "beamflow-private-server",
  "version": "3.0.0",
  "description": "BeamFlow Private Server with Strengthened System Components - Enhanced Edition",
  "private": true,
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "echo 'Backend build completed - Node.js server'",
    "build:frontend": "vite build",
    "build:full": "npm run build:frontend && npm run build",
    "test": "echo 'Tests skipped - no test framework configured'",
    "test:watch": "echo 'Tests skipped - no test framework configured'",
    "test:coverage": "echo 'Tests skipped - no test framework configured'",
    "lint": "echo 'Linting skipped - ESLint not configured'",
    "lint:fix": "echo 'Linting skipped - ESLint not configured'",
    "format": "prettier --write src/",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "ftp:start": "node src/ftp-server.js",
    "ssh:start": "node src/ssh-server.js",
    "frontend:dev": "vite",
    "frontend:build": "vite build",
    "frontend:preview": "vite preview",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist node_modules",
    "postinstall": "echo 'Post-install completed'",
    "db:setup": "node scripts/setup-database.js",
    "db:reset": "node scripts/setup-database.js --reset",
    "db:status": "node scripts/setup-database.js --status"
  },
  "keywords": [
    "beamflow",
    "server",
    "security",
    "performance",
    "monitoring",
    "cache",
    "authentication",
    "ftp",
    "ssh",
    "plugins",
    "react",
    "typescript",
    "modern-ui",
    "ai",
    "machine-learning",
    "real-time",
    "websockets"
  ],
  "author": "BeamFlow Team",
  "license": "MIT",
  "engines": {
    "node": ">=20.17.0",
    "npm": ">=10.0.0"
  },
