{
  "name": "docssitetemplate",
  "version": "1.0.0",
  "description": "BeamFlow Documentation Site",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vite preview",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write .",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:env": "tsx test-env.ts",
    "test:sw:versioning": "node scripts/test-sw-versioning.js",
    "test:security": "node scripts/test-security-headers.js",
    "test:security:local": "node scripts/test-security-headers.js http://localhost:3000",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "build:full": "npm run build && npm run build:backend",
    "build:backend": "cd _internal/system && npm run build",
    "build:secret": "node scripts/build-full-system.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "dev:backend": "cd _internal/system && npm run dev",
    "dev:socket": "cd server && npm run dev",
    "dev:realtime": "concurrently \"npm run dev\" \"npm run dev:socket\"",
    "start:backend": "cd _internal/system && npm start",
    "deploy": "npm run build:secret",
    "deploy:prepare": "node scripts/prepare-deployment.js",
    "deploy:github-pages": "npm run build && node scripts/deploy-github-pages.js",
    "deploy:vercel": "npm run build:secret && vercel --prod",
    "deploy:full": "npm run build:secret && vercel --prod",
    "deploy:all": "node scripts/deploy-all.js",
    "i18n:copy": "node scripts/copy-translations.js",
    "i18n:build": "npm run i18n:copy && npm run build",
    "i18n:dev": "npm run i18n:copy && npm run dev",
    "prepare": "echo 'Husky disabled for Windows compatibility'"
  },
  "keywords": [
    "documentation",
    "beamflow",
    "static-site",
    "github-pages"
  ],
  "author": "BeamFlow Team",
  "license": "MIT",
  "dependencies": {
    "caniuse-lite": "^1.0.30001737",
    "i18next": "^25.4.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "prom-client": "^15.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.7.2",
    "react-router-dom": "^6.8.0",
    "socket.io-client": "^4.7.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitejs/plugin-react": "^5.0.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "dotenv": "^17.2.1",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.5.6",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.1.1",
    "tsx": "^4.20.5",
    "vite": "^7.1.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Millsy102/docssitetemplate.git"
  },
  "bugs": {
    "url": "https://github.com/Millsy102/docssitetemplate/issues"
  },
  "homepage": "https://millsy102.github.io/docssitetemplate/"
}
