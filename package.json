{
  "name": "beamflow-docs",
  "version": "1.0.0",
  "description": "BeamFlow Documentation Site",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vite preview",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write .",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "deploy": "npm run build && npm run deploy:prepare",
    "deploy:prepare": "node scripts/prepare-deployment.js",
    "deploy:github-pages": "npm run build && node scripts/deploy-github-pages.js"
  },
  "keywords": [
    "documentation",
    "beamflow",
    "static-site",
    "github-pages"
  ],
  "author": "BeamFlow Team",
  "license": "MIT",
  "dependencies": {
    "caniuse-lite": "^1.0.30001737",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jest": "^29.5.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.3.0",
    "vite": "^4.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/beamflow-docs.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/beamflow-docs/issues"
  },
  "homepage": "https://yourusername.github.io/beamflow-docs/"
}
